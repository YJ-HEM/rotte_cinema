<Window x:Class="rotte_cinema.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:rotte_cinema"
        mc:Ignorable="d"
        Title="MainWindow" Height="504" Width="875" Loaded="Grid_Loaded">
    <Grid x:Name="grid" HorizontalAlignment="Stretch">
        <ComboBox x:Name="cmb_index" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="cmb_index_SelectionChanged"/>
    </Grid>
</Window>
